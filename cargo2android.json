{
  "dependencies": true,
  "device": true,
  "ignore-cargo-errors": true,
  "min-sdk-version": "29",
  "run": true,
  "variants": [
    {
      "features": "once,std",
      "tests": true
    },
    {
      "features": "mutex,spin_mutex",
      "force-rlib": true,
      "no-host": true,
      "suffix": "_nostd",
      "no-std": true
    }
  ]
}
